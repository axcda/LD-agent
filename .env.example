# API密钥配置
# OpenAI - 单个API密钥（向后兼容）
OPENAI_API_KEY=your_openai_api_key_here
# OpenAI - 多个API密钥（用逗号分隔，优先使用此项）
OPENAI_API_KEYS=your_openai_api_key_1,your_openai_api_key_2,your_openai_api_key_3
# OpenAI基础URL（可选，默认为https://api.openai.com/v1）
OPENAI_BASE_URL=https://api.openai.com/v1

# Google Gemini
GOOGLE_API_KEY=your_google_api_key_here

# 阿里百炼
ALIBABA_API_KEY=your_alibaba_api_key_here
ALIBABA_API_SECRET=your_alibaba_api_secret_here

# 其他配置
MAX_TOKENS=4000
TEMPERATURE=0.7

# 日志级别 (DEBUG, INFO, WARNING, ERROR)
LOG_LEVEL=DEBUG